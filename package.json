  "dependencies": {
    "@modelcontextprotocol/sdk": "1.0.1",
    "openai": "^4.76.0"
  },
  "devDependencies": {
    "shx": "^0.3.4",
    "tsx": "^4.19.2",
    "typescript": "^5.6.2"
  }
