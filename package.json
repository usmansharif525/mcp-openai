{
  "name": "@usmansharif/mcp-openai",
  "version": "0.0.1",
  "description": "MCP server for simple interaction with OpenAI API",
  "license": "MIT",
  "author": "usmansharif525",
  "homepage": "https://github.com/usmansharif525/mcp-openai",
  "repository": {
    "type": "git",
    "url": "https://github.com/usmansharif525/mcp-openai.git"
  },
  "bugs": "https://github.com/usmansharif525/mcp-openai/issues",
  "type": "module",
  "bin": {
    "mcp-server-webresearch": "dist/index.js"
  },
  "files": [
